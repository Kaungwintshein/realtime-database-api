{
  "version": 2,
  "builds": [
    { "src": "dist/main.js", "use": "@vercel/node" },
    { "src": "prisma/schema.prisma", "use": "@vercel/static-build" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ]
}
